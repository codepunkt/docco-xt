{
  "name":         "docco-xt",
  "description":  "Generates static project documentation. A fork of docco with focus on larger projects",
  "keywords":     ["documentation", "docs", "generator", "coffeescript"],
  "author":       "Christoph Werner <gonsfx@googlemail.com>",
  "version":      "0.1.1",
  "licenses": [{
    "type":       "MIT",
    "url":        "http://opensource.org/licenses/mit-license.php"
  }],
  "contributors": [
    "Christoph Werner <gonsfx@googlemail.com>",
    "Jeremy Ashkenas"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/gonsfx/docco-xt.git"
  },
  "engines": {
    "node":       ">=0.2.0"
  },
  "dependencies": {
    "commander":  ">= 0.5.2",
    "marked":     ">= 0.2.7",
    "fs-extra":   ">= 0.6.0",
    "underscore": ">= 1.0.0",
    "highlight.js": ">= 8.0.x"
  },
  "main" :        "./docco",
  "bin": {
    "docco":      "./bin/docco"
  }
}
